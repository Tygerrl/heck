	_G.autoclick = true;

	spawn(function()
		while wait() do
			if _G.autoclick == true then
				local args = {
					[1] = "Normal"
				}

				game:GetService("ReplicatedStorage").Remotes.Taps:FindFirstChild("2488796210"):FireServer(unpack(args))
			end
		end
	end)
end)

arON.Name = "arON"
arON.Parent = ScreenGui
arON.BackgroundColor3 = Color3.fromRGB(11, 213, 8)
arON.Position = UDim2.new(0.81070286, 0, 0.734969318, 0)
arON.Size = UDim2.new(0, 87, 0, 35)
arON.Font = Enum.Font.SourceSans
arON.Text = "On"
arON.TextColor3 = Color3.fromRGB(0, 0, 0)
arON.TextSize = 14.000
arON.MouseButton1Down:connect(function()
	_G.autorebirth = true;
	
	spawn(function()
		while wait () do
			if _G.autorebirth == true then
				local args = {
					[1] = 500
				}

				game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer(unpack(args))
			end
		end
	end)
end)

arOFF.Name = "arOFF"
arOFF.Parent = ScreenGui
